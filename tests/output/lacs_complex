def main(a: Int, b: Int): Int = {
	reduce(map(naturals(), square), add, 10)
}

def add(a: Int, b: Int): Int = {
	a + b
}

def multiply(a: Int, b: Int): Int = {
	a * b
}

def map(stream: () => Int, f: (Int) => Int): () => Int = {
	def next(): Int = {
		f(stream())
	}

	next
}

def square(i: Int): Int = {
	i * i
}

def naturals(): () => Int = {
	var i: Int;

	def next(): Int = {
		i = i + 1;
		i
	}

	i = 0;
	next
}

def reduce(stream: () => Int, op: (Int, Int) => Int, count: Int): Int = {
	if (count <= 1) {
		stream()
	} else {
		op(stream(), reduce(stream, op, count - 1))
	}
}